grammar com.aelos.MappingAssistance with org.eclipse.xtext.xbase.Xbase

generate mappingassistance "http://www.aelos.com/MappingAssistance"

Heros:
	heros+=SuperHero*;
	
SuperHero:
	'hero' name=ID 'can' abilities+=Ability (someCode=XBlockExpression)?;
	
enum Ability :
	FLY | SUPERPOWER | FIRE | ICE
;
