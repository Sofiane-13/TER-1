grammar com.aelos.TestIntentionsAssistance with org.eclipse.xtext.xbase.Xbase

generate testintentionsassistance "http://www.aelos.com/TestIntentionsAssistance"

Heros:
	heros+=SuperHero*;
	
SuperHero:
	'hero' name=ID 'can' abilities+=Ability (someCode=XBlockExpression)?;
	
enum Ability :
	FLY | SUPERPOWER | FIRE | ICE
;
