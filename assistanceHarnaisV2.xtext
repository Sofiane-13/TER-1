Model:
	function+=AbstractModel*;


AbstractModel:	Component map+=Mapping* bind+=Bindings*;

Component :
	"Component:" name=ID
	"ProvidedServices:"
		"conf("(arg+=Variable ",")*arg1+=Variable")"
		(methode+=ServiceName "("(arg+=Variable ",")*arg+=Variable")"(":"arg+=Variable)?)*
	"RequiredServices:"
		(nameComp+=[Component]'.'nameServ+=[ServiceName])*
;

Mapping  :
	 
	'varModel:' nameVarMode+=[Variable] 'ref' 'varTest:' nameVarTest+=[Variable]
	
;

Bindings :
	"bind(" nameComp+=[Component]'.'nameServ+=[ServiceName] "," nameComp+=[Component]'.'nameServ+=[ServiceName] ")"  
;

ServiceName : name=ID;

Variable:
      name=ID ":" type=Type;
      
enum Type : INT | STRING | Boolean | Double;
